# Global definition file
# shared by both C and NASM sources

# Constant returned by FreeCOM to force respawner module to extecute
# an external program
constant FREECOM_ERRLVL_SPAWN_EXTERNAL 0x73

# Constant returned by FreeCOM to terminate the current session of
# FreeCOM
constant FREECOM_ERRLVL_TERMINATE	0x6b
